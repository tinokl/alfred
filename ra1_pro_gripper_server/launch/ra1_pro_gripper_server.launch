<?xml version="1.0" encoding="utf-8"?>
<launch>

  <!-- Load the URDF, SRDF and other .yaml configuration files on the param server -->
  <include file="$(find ra1_pro_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>

  <group ns="robot">
    <!-- Load gripper controller -->
    <node name="ra1_pro_gripper_server" pkg="ra1_pro_gripper_server" type="gripper_action_server"
	  respawn="false" output="screen" />
  </group>

  <!-- Publish the gripper joint states to /tf 
  <include file="$(find baxter_state_publisher)/launch/baxter_state_publisher.launch"/>
  -->

</launch>
